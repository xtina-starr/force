.consignments-faq
  .consignments-faq__modal
    .consignments-faq__contents
      .consignments-faq__close
        include ../../../components/main_layout/public/icons/close.svg
      .consignments-faq__header
        = sections.faq.headline
      for item in sections.faq.items
        .consignments-faq__item
          .consignments-faq__item__question
            = item.question
          .consignments-faq__item__answer
            = item.answer
      .consignments-faq__contact-us
        | Need more assistance?&nbsp;
        a( href='mailto:consignments@artsy.net' )
          | Contact us
